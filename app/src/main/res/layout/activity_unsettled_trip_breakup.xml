<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_unsettled_trip_breakup"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#afa9a9">
        <TextView android:id="@+id/txtUnTripVehicleNo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:layout_marginBottom="11dp"
            android:gravity="center"
            android:text="VehNo"
            android:textSize="15dp"
            android:textStyle="bold"
            android:textColor="@color/Black"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#53dedddc"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text="Triplog No"
            android:textSize="15sp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textColor="@color/Black"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textColor="@color/Black" />

        <TextView
            android:id="@+id/untriplogno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.9"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:text="0"
            android:textColor="@color/Black"
            android:layout_marginLeft="10dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#53dedddc"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text="From date-To Date"
            android:textSize="15sp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textColor="@color/Black"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textColor="@color/Black"
                />

        <TextView
            android:id="@+id/unsettledFromDate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.9"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:text="0"
            android:textColor="@color/Black"
            android:layout_marginLeft="10dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#53dedddc"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text="Party"
            android:textSize="15sp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textColor="@color/Black"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textColor="@color/Black"
            />

        <TextView
            android:id="@+id/unParty"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.9"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:text="0"
            android:textColor="@color/Black"
            android:layout_marginLeft="10dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#53dedddc"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text="KM"
            android:textSize="15sp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textColor="@color/Black"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textColor="@color/Black"
            />

        <TextView
            android:id="@+id/unKM"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.9"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:text="0"
            android:textColor="@color/Black"
            android:layout_marginLeft="10dp"
           />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#53dedddc"
        android:orientation="horizontal">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text="Driver"
            android:textSize="15sp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textColor="@color/Black"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:textSize="20sp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textColor="@color/Black"
            />

        <TextView
            android:id="@+id/unDriver"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.9"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="6dp"
            android:layout_marginTop="6dp"
            android:text="0"
            android:layout_marginLeft="10dp"
            android:textColor="@color/Black"

              />
    </LinearLayout>

    <LinearLayout android:id="@+id/unsettledBreakupAmtQyt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#c6bebe">
            <View
                android:layout_width="2dp"
                android:layout_height="40dp"
                android:background="#ffffff"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Type"
                android:layout_weight="0.9"
                android:layout_gravity="center"
                android:layout_marginLeft="7dp"
                android:textStyle="bold"
                android:textColor="@color/Black"
                android:textSize="15dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:layout_weight="1"
                 >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Budget"
                    android:gravity="end"
                    android:textStyle="bold"
                    android:layout_gravity="center"
                    android:textColor="@color/Black"
                    android:textSize="15dp"/>
            </LinearLayout>



            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Actual"
                android:textStyle="bold"
                android:layout_gravity="center"
                android:gravity="end"
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="15dp"/>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.9"
                android:layout_gravity="center"
                android:gravity="end">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Variance"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="15dp"
                    android:textStyle="bold"
                    android:textColor="@color/Black"
                    android:textSize="15dp"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#e2e0e0">
            <View
                android:layout_width="2dp"
                android:layout_height="40dp"
                android:background="#ffffff"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="FUEL"
                android:layout_weight="0.9"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textColor="@color/Black"
                android:textSize="15dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:layout_weight="1">

                <TextView android:id="@+id/unSetFuelBud"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold"
                    android:gravity="end"
                    android:layout_gravity="center"
                    android:textColor="@color/Black"
                    android:textSize="15dp"/>
            </LinearLayout>



            <TextView android:id="@+id/unSetFuelActual"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0"
                android:textStyle="bold"
                android:layout_gravity="center"
                android:gravity="end"
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="15dp"/>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.9"
                android:gravity="end"
                android:layout_gravity="center">

                <TextView android:id="@+id/unSetFuelVariance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="15dp"
                    android:textStyle="bold"
                    android:textColor="@color/Black"
                    android:textSize="15dp"/>
            </LinearLayout>

        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_height="2dp"
            android:background="#ffffff"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#c6bebe"
            android:visibility="gone">
            <View
                android:layout_width="2dp"
                android:layout_height="40dp"
                android:background="#ffffff"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Amount"
                android:layout_weight="0.9"
                android:layout_gravity="center"
                android:layout_marginLeft="7dp"
                android:textStyle="bold"
                android:textColor="@color/Black"
                android:textSize="15dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:layout_weight="1"
                >

                <TextView android:id="@+id/unSetCashBud"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Budget"
                    android:gravity="end"
                    android:textStyle="bold"
                    android:layout_gravity="center"
                    android:textColor="@color/Black"
                    android:textSize="15dp"/>
            </LinearLayout>



            <TextView android:id="@+id/unSetCashActual"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Actual"
                android:textStyle="bold"
                android:layout_gravity="center"
                android:gravity="end"
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="15dp"/>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.9"
                android:layout_gravity="center"
                android:gravity="end">

                <TextView android:id="@+id/unSetCashVariance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Variance"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="15dp"
                    android:textStyle="bold"
                    android:textColor="@color/Black"
                    android:textSize="15dp"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="#e2e0e0">
            <View
                android:layout_width="2dp"
                android:layout_height="40dp"
                android:background="#ffffff"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="CASH"
                android:layout_weight="0.9"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textColor="@color/Black"
                android:textSize="15dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:layout_weight="1">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold"
                    android:gravity="end"
                    android:layout_gravity="center"
                    android:textColor="@color/Black"
                    android:textSize="15dp"
                    android:id="@+id/textView21" />
            </LinearLayout>



            <TextView android:id="@+id/ActualCash"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0"
                android:textStyle="bold"
                android:layout_gravity="center"
                android:gravity="end"
                android:layout_marginLeft="7dp"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:textSize="15dp"/>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.9"
                android:gravity="end"
                android:layout_gravity="center">

                <TextView android:id="@+id/cashVariance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="15dp"
                    android:textStyle="bold"
                    android:textColor="@color/Black"
                    android:textSize="15dp"/>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>
